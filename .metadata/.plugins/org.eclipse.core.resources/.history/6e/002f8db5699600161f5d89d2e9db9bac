package com.celera.message.cmmf;

public enum EMessageType implements GenericEnum
{
	QUERY_ALL_TRADES('A'), QUERY_TRADE_BETWEEN('B');
	
	private final Character asChar;
	
	public boolean equals(char asChar) {
		return this.asChar == asChar;
	}
	
	public String asChar() 
	{
		return Character.toString(asChar);
	}

	private EMessageType(char asChar) {
		this.asChar = asChar;
	}
	
    public static EMessageType get(final char asChar)
    {
        for (EMessageType v : EMessageType.values())
            if (v.asChar == asChar)
                return v;
        return null;
    }
}
