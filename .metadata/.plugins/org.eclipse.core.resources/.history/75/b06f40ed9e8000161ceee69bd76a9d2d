package com.vectails.data;

import java.util.ArrayList;
import java.util.List;

import org.w3c.dom.Element;
import org.w3c.dom.Node;
import org.w3c.dom.NodeList;

public class ElementList {
	
	private List<IndexFuture> myList = new ArrayList<IndexFuture>();
	
	public void parseXmlElement(Element el) {
//      List<Currency> l = new ArrayList<Currency>();
      
		NodeList nl = el.getChildNodes();
		int len = nl.getLength();
		for (int i =0 ; i<len; i++) {
			Node n = nl.item(i);
			if (n.getNodeType() == Node.ELEMENT_NODE) {
				IndexFuture o = new IndexFuture();
				o.parseAttribute((Element)n);
	
		        myList.add(o);
		        
		        System.out.println(o);
			}
		}
	}
}
