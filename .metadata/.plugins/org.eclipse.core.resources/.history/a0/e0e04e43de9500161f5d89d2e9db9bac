package com.celera.message.cmmf;

interface ICharEnum<T extends Enum>
{
	final char asChar = ' ';
	
    public static <T extends Enum<T>> T getEApp(Class<T> enumType, final char asChar)
    {
        for (T v : enumType.getEnumConstants())
            if (v.equals(asChar))
                return v;
        return null;
    }
}
