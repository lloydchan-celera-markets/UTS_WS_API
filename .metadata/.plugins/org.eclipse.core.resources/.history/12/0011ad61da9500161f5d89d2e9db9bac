package com.celera.message.cmmf;

public enum EApp
{
	DBA('D');
	
	private final char asChar;
	
	public boolean equals(char asChar) {
		return this.asChar == asChar;
	}
	
	public String asChar() 
	{
		return Character.toString(asChar);
	}
	
	private EApp(char asChar) {
		this.asChar = asChar;
	}
	
    public static EApp getEApp(final char asChar)
    {
        for (EApp v : EApp.values())
            if (v.asChar == asChar)
                return v;

        return null;
    }
}
