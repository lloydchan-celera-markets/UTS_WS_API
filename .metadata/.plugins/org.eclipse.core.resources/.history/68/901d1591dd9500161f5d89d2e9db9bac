package com.celera.message.cmmf;

interface ICharEnum<T extends Enum>
{
	final char asChar = ' ';
	
    public static <T extends Enum<T>> T getEApp(final char asChar)
    {
    	Class<T> enumType;
    	
        for (T v : enumType.getEnumConstants())
            if (v.asChar == asChar)
                return v;
        return null;
    }
}
