package com.celera.message.cmmf;

public interface GenericEnum<T>
{
    public static <T extends Enum<T>> T get(Class T, final char asChar)
    {
    	Class<T> clazz = GenericEnum.class.getEnumConstants();
        for (T v : T..class.values())
            if (v.asChar == asChar)
                return v;
        return null;
    }
}
