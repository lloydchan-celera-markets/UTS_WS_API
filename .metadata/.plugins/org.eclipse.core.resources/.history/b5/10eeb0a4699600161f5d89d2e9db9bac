package com.celera.message.cmmf;

public enum EApp
{
	DBA('D'), WEB_UI('W');
	
	private final Character asChar;
	
	public boolean equals(char asChar) {
		return this.asChar == asChar;
	}
	
	public String asChar() 
	{
		return Character.toString(asChar);
	}

	private EApp(char asChar) {
		this.asChar = asChar;
	}
	
    public static EApp get(final char asChar)
    {
        for (EApp v : EApp.values())
            if (v.asChar == asChar)
                return v;
        return null;
    }

    public static EApp get(final byte asChar)
    {
    	for (EApp v : EApp.values())
    		if (v.asChar == asChar)
    			return v;
    	return null;
    }
}
