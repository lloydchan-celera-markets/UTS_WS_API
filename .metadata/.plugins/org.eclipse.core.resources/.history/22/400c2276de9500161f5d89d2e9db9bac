package com.celera.message.cmmf;

public interface ICharEnum<T extends Enum>
{
	final char asChar = ' ';
	
	public boolean equals(char asChar) {
		return this.asChar == asChar;
	}
	
	public default String asChar() 
	{
		return Character.toString(asChar);
	}
	
    public static <T extends Enum<T>> T getEApp(Class<T> enumType, final char asChar)
    {
        for (T v : enumType.getEnumConstants())
            if (v.equals(asChar))
                return v;
        return null;
    }
}
