package com.vectails.data;

import java.util.ArrayList;
import java.util.List;

import org.w3c.dom.Element;
import org.w3c.dom.Node;
import org.w3c.dom.NodeList;

public class Currencies {
	
	private List<Currency> myList = new ArrayList<Currency>();
	
	public void parseXmlElement(Element el) {
//      List<Currency> l = new ArrayList<Currency>();
      
		NodeList nl = el.getChildNodes();
		int len = nl.getLength();
		for (int i =0 ; i<len; i++) {
			Node n = nl.item(i);
			if (n.getNodeType() == Node.ELEMENT_NODE) {
				Currency curncy = new Currency();
				curncy.parseAttribute((Element)n);
//				NamedNodeMap m = nl.item(i).getAttributes();
//		        String code = m.getNamedItem(IXmlTag.Code).getNodeValue();
//		        String name = m.getNamedItem(IXmlTag.Name).getNodeValue();
	
		        System.out.print(curncy);
		        
		        myList.add(curncy);
			}
		}
		
//      return l;
	}
}
