package com.celera.message.cmmf;

public interface GenericEnum<T>
{
	public boolean equals(char asChar);
	
	public static <T extends GenericEnum<T>> Class<T> clazz()
	{
		return GenericEnum.<T>class;
	}
	
//    public static <T extends GenericEnum<T>> T get(Class<T> clazz, final char asChar)
    public static <T extends GenericEnum<T>> T get(final char asChar)
    {
    	Class<T> clazzes = GenericEnum.clazz();
        for (T v : clazzes.getEnumConstants())
            if (v.equals(asChar))
                return (T)v;
        return null;
    }
}
