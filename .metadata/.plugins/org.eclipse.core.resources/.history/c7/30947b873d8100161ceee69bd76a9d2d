package com.vectails.data;

import java.util.ArrayList;
import java.util.List;

import org.w3c.dom.Element;
import org.w3c.dom.Node;
import org.w3c.dom.NodeList;

public class Currencies2 {
	
	private List<Currency2> myList = new ArrayList<Currency2>();
	
	@Override
	public String toString() {
		StringBuilder sb = new StringBuilder();
		sb.append("Currencies [");
		for (Currency2 l: myList) {
			sb.append(l.toString()).append(", ");
		}
		sb.append("]");
		return sb.toString();
	}
	
	public void parseXmlElement(Element el) {
		NodeList nl = el.getChildNodes();
		int len = nl.getLength();
		for (int i =0 ; i<len; i++) {
			Node n = nl.item(i);
			if (n.getNodeType() == Node.ELEMENT_NODE) {
				Currency2 o = new Currency2();
				o.parseAttribute((Element)n);
		        o.parseLeaveNodes((Element)n);
		        myList.add(o);
			}
		}
	}
}
