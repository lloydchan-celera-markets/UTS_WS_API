package com.celera.message.cmmf;

import java.util.UUID;

public abstract class CmmfApp
{
	String uniqueID = UUID.randomUUID().toString();

	private EApp me;

	public CmmfApp(EApp me)
	{
		this.me = me;
	}

	public boolean isMine(char me)
	{
		return this.me.equals(me);
	}

	public String id()
	{
		return uniqueID;
	}
	
	public abstract void onQuery(byte[] data);
	public abstract void onAdmin(byte[] data);
	public abstract void onAnswer(byte[] data);

}
