package com.vectails.data;

import org.apache.cxf.common.i18n.Exception;
import org.w3c.dom.Element;
import org.w3c.dom.NamedNodeMap;
import org.w3c.dom.Node;
import org.w3c.dom.NodeList;

public class SubscribedUnderlyings implements IXmlParser{
	private String Code;
	
	SubscribedUnderlyings myList = new SubscribedUnderlyings();
	
	@Override
	public String toString() {
		return "DerivativeType [code=" + Code + ", " + myList.toString()	+ "]";
	}
	
	public void setCode(String code) {
		Code = code;
	}

	public void parseXmlElement(Element root) {

		this.parseAttribute(root);

		NodeList nl = root.getChildNodes();
		int lenNl = nl.getLength();
		for (int i = 0; i < lenNl; i++) {
			Node n1 = nl.item(i);
			if (n1.getNodeType() == Node.ELEMENT_NODE
					&& IXmlTag.SubscribedUnderlyings.toString().equals(n1.getNodeName())) {
					myList.parseXmlElement((Element) n1);
			}
		}
		System.out.println(this.toString());
	}
}
