package com.celera.message.cmmf;

public interface GenericEnum
{
	public boolean equals(char asChar);
	
    public static <T extends GenericEnum> T get(Class<T> clazz, final char asChar)
    {
        for (T v : clazz.getEnumConstants())
            if (v.equals(asChar))
                return v;
        return null;
    }
}
